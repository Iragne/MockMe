doctype 5
html
  head
    title MockMe by Adelskott
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    link(rel='stylesheet', href='/css/global.css')
    link(rel='stylesheet', href='/css/bootstrap.css')
    link(rel='stylesheet', href='/css/bootstrap-responsive.css')
    link(rel='stylesheet', href='/css/datetimepicker.css')

    
    block css      

    script(src='http://code.jquery.com/jquery-1.7.2.min.js')
    script(src='/socket.io/socket.io.js')

    
    script(src='/js/lib/underscore-min.js')
    script(src='/js/lib/require.js')
    
    script(src='/js/lib/backbone-min.js')
    

    
    script(src='/js/lib/jquery-ui.js')
    script(src='/js/lib/bootstrap.min.js')

    script(src='/js/lib/bootstrap-datetimepicker.min.js')
    script(src='/js/lib/locales/bootstrap-datetimepicker.fr.js')
    script(src='/js/lib/date.js')    

    block scriptadd

    body
        .navbar.navbar-inverse.navbar-fixed-top
            .navbar-inner
                .container
                    a.brand(href="/") Mock
                    .nav-collapse.collapse
        div(id='jai-content')
           #content  
              block content
        div(id='jai-footer')
               div(class='f-left') &copy;  
                  a(href='https://github.com/Iragne') Adelskott
               div(class='f-right') Powered by Adelskott


 